game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
if game:GetService("ReplicatedStorage").GameData.Floor.Value == "Hotel" then
if game.ReplicatedStorage.GameData.LatestRoom.Value == 50 then

---====== Load spawner ======---

local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/Voorproo/Fogroom/refs/heads/main/Hello"))()

---====== Create entity ======---

local entity = spawner.Create({
	Entity = {
		Name = "Common Sense",
		Asset = "https://github.com/PABMAXICHAC/doors-monsters-models/raw/main/Common_Sense2.rbxm",
		HeightOffset = 1
	},
	Lights = {
		Flicker = {
			Enabled = true,
			Duration = 1
		},
		Shatter = true,
		Repair = false
	},
	Earthquake = {
		Enabled = false
	},
	CameraShake = {
		Enabled = false,
		Range = 100,
		Values = {1.5, 20, 0.1, 1} -- Magnitude, Roughness, FadeIn, FadeOut
	},
	Movement = {
		Speed = 45,
		Delay = 5,
		Reversed = false
	},
	Rebounding = {
		Enabled = true,
		Type = "Ambush", -- "Blitz"
		Min = 100,
		Max = 100,
		Delay = 0.001
	},
	Damage = {
		Enabled = true,
		Range = 15,
		Amount = 0.01
	},
	Crucifixion = {
		Enabled = true,
		Range = 80,
		Resist = false,
		Break = true
	},
	Death = {
		Type = "Guiding", -- "Curious"
		Hints = {"Death", "Hints", "Go", "Here"},
		Cause = ""
	}
})

---====== Debug entity ======---

entity:SetCallback("OnSpawned", function()
    print("Entity has spawned")
if game.Workspace:WaitForChild("RepentanceNEW", 750) then
function GitAud(soundgit,filename)
    SoundName=tostring(SoundName)
    local url=soundgit
    local FileName = filename
    writefile(FileName..".mp3", game:HttpGet(url))
    return (getcustomasset or getsynasset)(FileName..".mp3")
end

function CustomGitSound(soundlink, vol, filename)
    local sound = Instance.new("Sound")
    sound.SoundId = GitAud(soundlink, filename)
    sound.Parent = workspace
    sound.Volume = 3
   sound:Play()
end

CustomGitSound("https://raw.githubusercontent.com/Voorproo/Eye/refs/heads/main/DOORS-UNUSED-SOUNDTRACK-Stress%20(mp3cut.net).mp3", 1, "crucifix")
wait(5)
local achievementGiver = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()

---====== Display achievement ======---
achievementGiver({
    Title = "Im just use my Common Sense.",
    Desc = "Let me find the book alone.",
    Reason = "Use Crucifix on Common Sense",
    Image = "rbxassetid://139528160080039"
})

game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
game.Workspace.CommonSense:Destroy()
---====== Load achievement giver ======---
local achievementGiver = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()

---====== Display achievement ======---
achievementGiver({
    Title = "Survival from Common Sence",
    Desc = "So smoke.",
    Reason = "Encounter Common Sence",
    Image = "rbxassetid://139528160080039"
}) 
end
end)

entity:SetCallback("OnDamagePlayer", function(newHealth)
	if newHealth == 0 then
		print("Entity has killed the player")
	else
		print("Entity has damaged the player")
		game.Players.LocalPlayer.Character.Humanoid:TakeDamage(1000)
        game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Common Sense"
	end
end)
---====== Run entity ======---
entity:Run()
end
end
end)
